{
  "manifest_version": 3,
  "name": "Vytal",
  "version": "2.0.0",
  "description": "Spoof your location data and user agent.",
  "permissions": ["storage", "debugger", "activeTab", "alarms"],
  "background": { "service_worker": "background.bundle.js" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-32.png"
  },
  "icons": {
    "128": "icon-128.png"
  }
}
